<h1>検索結果</h1>

<table class="table table-striped">
  <thread>
    <tr>
    <th>部屋の名前</th>
    <th>投稿日</th>
    <th>市区町村</th>
    </tr>
  </thread>
    <% @search.each do |post| %>
    <tbody>
    <td><%= post.room_name %></td>
    <td><%= post.created_at %></td>
    <td><%= post.city %></td>
    </tbody>
    <% end %>
</table>


